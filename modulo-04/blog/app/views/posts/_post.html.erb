<div id="<%= dom_id post %>">
  <p>
    <strong>Título do livro:</strong>
    <%= post.title %>
  </p>
    <p>
    <strong>Autor do livro:</strong>
    <%= post.autor %>
  </p>
    <p>
    <strong>Nota:</strong>
    <%= post.nota %>
  </p>

  <p>
    <strong>Descrição do livro:</strong>
    <%= post.description %>
  </p>

  <p>
    <strong>Text:</strong>
    <%= post.text %>
  </p>

</div>
